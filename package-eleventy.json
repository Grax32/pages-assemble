{
  "name": "pages-assemble-eleventy",
  "version": "1.0.0",
  "description": "Static site built with 11ty (Eleventy)",
  "scripts": {
    "clean": "rimraf static-basedir/build",
    "build": "npx @11ty/eleventy",
    "start": "npx @11ty/eleventy --serve",
    "serve": "npx @11ty/eleventy --serve",
    "debug": "npx @11ty/eleventy --dryrun",
    "netlify": "npm run build"
  },
  "keywords": [
    "static",
    "eleventy",
    "11ty"
  ],
  "author": "David Walker <dwalker-git@grax32.com> (https://grax32.com/)",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-navigation": "^0.3.5",
    "gray-matter": "^4.0.3",
    "markdown-it": "^14.0.0",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "prettier": "^3.2.4"
  }
}
